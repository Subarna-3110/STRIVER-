class Solution {
    // Function for finding maximum and value pair
    public static int lenOfLongSubarr(int A[], int N, int K) {
        // Complete the function
        int max=0;
        for(int i=0;i<N;i++)
        {
            int currentsum=0;
            for(int j=i;j<N;j++)
            {
                currentsum+=A[j];
                if(currentsum==K)
                {
                    int length=j-i+1;
                    max=Math.max(max,length);
                }
            }
        }
        return max;
    }
}
